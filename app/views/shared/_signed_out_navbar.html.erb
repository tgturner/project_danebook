<header>
  <nav class="navbar navbar-default sign_up_nav">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a id="logo" class="navbar-brand" href="#">Danebook</a>
      </div>

      <div class="collapse navbar-collapse" id="navbar">
        <form class="navbar-form navbar-right sign_up_nav_form" action="<%= session_path %>" method="post">
          <fieldset class="form-group">
            <label for="db-email">Email</label>
            <br>
              <input name="email" id="db-email" type="email" class="form-control" placeholder="Email">
          </fieldset>
          <fieldset class="form-group">
            <label for="db-password">Password</label>
            <br>
            <input name="password" id="db-password" type="password" class="form-control" placeholder="Password">
          </fieldset>
          <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>" >
        <button type="submit" class="btn btn-primary sign_in_button">Log In</button>
        </form>
      </div>
    </div>
  </nav>
</header>
